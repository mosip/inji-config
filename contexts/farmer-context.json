{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "type": "@type",
    "schema": "https://schema.org/",
    "FarmerCredential": {
      "@id": "https://mosip.github.io/inji-config/contexts/farmer-context.json#FarmerCredential"
    },
    "farmerDetails": "schema:Thing",
    "authority": {
      "@id": "schema:Text",
      "@container": "@language"
    },
    "idType": {
      "@id": "schema:Text",
      "@container": "@language"
    },
    "fullName": {
      "@id": "schema:name",
      "@container": "@language"
    },
    "farmerId": "schema:Text",
    "gender": {
      "@id": "schema:gender",
      "@container": "@language"
    },
    "phoneNumber": "schema:telephone",
    "dateOfBirth": "schema:birthDate",
    "address": {
      "@id": "schema:address",
      "@context": {
        "village": {
          "@id": "schema:addressLocality",
          "@container": "@language"
        },
        "district": {
          "@id": "schema:addressRegion",
          "@container": "@language"
        },
        "state": {
          "@id": "schema:addressRegion",
          "@container": "@language"
        }
      }
    },
    "primaryCommodity": {
      "@id": "schema:productSupported",
      "@container": "@language"
    },
    "totalLandArea": {
      "@id": "schema:area",
      "@context": {
        "value": "schema:value",
        "unit": {
          "@id": "schema:unitText",
          "@container": "@language"
        }
      }
    },
    "ownershipType": {
      "@id": "schema:Text",
      "@container": "@language"
    },
    "crops": {
      "@id": "schema:productSupported",
      "@container": "@list",
      "@context": {
        "cropName": {
          "@id": "schema:name",
          "@container": "@language"
        },
        "season": {
          "@id": "schema:temporalCoverage",
          "@container": "@language"
        }
      }
    },
    "face": "schema:Text"
  }
}
