{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "type": "@type",
    "schema": "https://schema.org/",
    "FarmerCredential": {
      "@id": "https://mosip.github.io/inji-config/contexts/farmer-context.json#FarmerCredential"
    },
    "farmerDetails": "schema:Thing",
    "authority": "schema:Text",
    "idType": "schema:Text",
    "fullName": "schema:name",
    "farmerId": "schema:Text",
    "gender": "schema:gender",
    "phoneNumber": "schema:telephone",
    "dateOfBirth": "schema:birthDate",
    "address": {
      "@id": "schema:address",
      "@context": {
        "village": "schema:addressLocality",
        "district": "schema:addressRegion",
        "state": "schema:addressRegion"
      }
    },
    "primaryCommodity": "schema:productSupported",
    "totalLandArea": {
      "@id": "schema:area",
      "@context": {
        "value": "schema:value",
        "unit": "schema:unitText"
      }
    },
    "ownershipType": "schema:Text",
    "crops": {
      "@id": "schema:productSupported",
      "@container": "@list",
      "@context": {
        "cropName": "schema:name",
        "season": "schema:temporalCoverage"
      }
    },
    "face": "schema:Text"
  }
}